<?xml version="1.0" encoding="utf-8"?>
<Optimizer type="CmaOptimizer">
	<INCLUDE file="optimizations/O_CMA.xml" merge_children="1"/>
	<max_generations>1000</max_generations>
	<maximize_objective>1</maximize_objective>
	<Objective type="SimulationObjective">
		<max_duration>1.0</max_duration>
		<Model type="Simbody">
			<signature_postfix></signature_postfix>
			<model_file>f1024.osim</model_file>
			<!--model_file>f1024.osim</model_file-->
			<integration_method>SemiExplicitEuler2</integration_method>
			<!--integration_method>RungeKuttaMerson</integration_method-->
			<integration_accuracy>0.0001</integration_accuracy>
			<use_fixed_control_step_size>0</use_fixed_control_step_size>
			<Controllers>
				<Controller type="FeedForwardController">
					<use_symmetric_actuators>1</use_symmetric_actuators>
					<Function type="PieceWiseConstantFunction">
						<control_points>2</control_points>
						<control_point_y init_min="0.25" init_max="0.75" min="0" max="1"/>
						<control_point_delta_time init_mean="0.2" init_std="0.02" is_free="1"/>
					</Function>
				</Controller>
				<INCLUDE file="measures/M_LongJump.xml"/>
			</Controllers>
		</Model>
	</Objective>
</Optimizer>

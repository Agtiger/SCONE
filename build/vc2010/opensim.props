<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<ImportGroup Label="PropertySheets" />
	<PropertyGroup Label="UserMacros">
		<OPENSIM_DEPENDENCIES Condition="'$(Configuration)|$(Platform)'=='Debug|Win32'" >osimSimulation_d.lib;osimActuators_d.lib;osimCommon_d.lib;osimAnalyses_d.lib;osimTools_d.lib;SimTKcommon_d.lib;SimTKmath_d.lib;SimTKsimbody_d.lib;</OPENSIM_DEPENDENCIES>
		<OPENSIM_DEPENDENCIES Condition="'$(Configuration)|$(Platform)'=='Release|Win32'" >osimSimulation.lib;osimActuators.lib;osimCommon.lib;osimAnalyses.lib;osimTools.lib;SimTKcommon.lib;SimTKmath.lib;SimTKsimbody.lib;</OPENSIM_DEPENDENCIES>
	</PropertyGroup>

	<ItemDefinitionGroup>
		<ClCompile>
			<AdditionalIncludeDirectories>$(OPENSIM_DEV_HOME)\sdk\include\SimTK\include;$(OPENSIM_DEV_HOME)\sdk\include;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
		</ClCompile>
		<Link>
			<AdditionalLibraryDirectories>$(OPENSIM_DEV_HOME)/sdk/lib;$(OPENSIM_DEV_HOME)/bin;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
			<AdditionalDependencies>$(OPENSIM_DEPENDENCIES);%(AdditionalDependencies)</AdditionalDependencies>
		</Link>
	</ItemDefinitionGroup>
</Project>
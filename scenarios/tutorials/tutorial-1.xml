<?xml version="1.0" encoding="utf-8"?>
<Optimizer type="CmaOptimizer">
	<max_generations>1000</max_generations>
	<maximize_objective>1</maximize_objective>
	<Objective type="SimulationObjective">
		<max_duration>2</max_duration>
		<Model type="Simbody">
			<model_file>f1020.osim</model_file>
			<integration_method>SemiExplicitEuler2</integration_method>
			<integration_accuracy>0.001</integration_accuracy>
			<Controllers>
				<Controller type="FeedForwardController">
					<use_symmetric_actuators>1</use_symmetric_actuators>
					<Function type="Polynomial">
						<degree>2</degree>
						<coefficient0 init_mean="0.25" init_std="0.1" min="0" max="1"/>
						<coefficient1 init_mean="0" init_std="1" min="-10" max="10"/>
						<coefficient2 init_mean="0" init_std="1" min="-10" max="10"/>
					</Function>
				</Controller>
				<Measure type="HeightMeasure">
					<terminate_on_peak>1</terminate_on_peak>
					<termination_height>0.5</termination_height>
					<ignore_time>0.2</ignore_time>
					<signature_postfix></signature_postfix>
				</Measure>
			</Controllers>
		</Model>
	</Objective>
</Optimizer>

<?xml version="1.0" encoding="utf-8"?>
<Optimizer type="CmaOptimizer">
	<max_generations>1000</max_generations>
	<maximize_objective>1</maximize_objective>
	<Objective type="SimulationObjective">
		<max_duration>2</max_duration>
		<Model type="Simbody">
			<model_file>f1020.osim</model_file>
			<state_init_optimization symmetric="1" init_std="0.01" min="-0.1" max="0.1" include_states="*" exclude_states="*_tx;*_ty;*_u;*.activation;*.fiber_length"/>
			<integration_method>SemiExplicitEuler2</integration_method>
			<integration_accuracy>0.001</integration_accuracy>
			<Controllers>
				<Controller type="FeedForwardController">
					<use_symmetric_actuators>1</use_symmetric_actuators>
					<Function type="PieceWiseConstantFunction">
						<control_points>3</control_points>
						<control_point_y init_mean="0.2" init_std="0.1" min="0" max="1"/>
						<control_point_delta_time init_mean="0.2" init_std="0.01" is_free="1"/>
					</Function>
				</Controller>
				<Measure type="JumpMeasure">
					<terminate_on_peak>1</terminate_on_peak>
					<termination_height>0.75</termination_height>
					<prepare_time>0.2</prepare_time>
					<jump_type>2</jump_type>
					<target_body>calcn_l</target_body>
				</Measure>
			</Controllers>
		</Model> 
	</Objective>
</Optimizer>

<?xml version="1.0" encoding="utf-8"?>
<Optimizer type="CmaOptimizer">
	<init_file>initializations/f0914_walk_N0MAB_NSI_S10.par</init_file>
	<init_file_std_factor>0.1</init_file_std_factor>
	<init_file_std_offset>0.01</init_file_std_offset>
	<use_init_file>1</use_init_file>
	<random_seed>123</random_seed>
	<max_generations>3000</max_generations>
	<Objective type="ImitationObjective">
		<signature_prefix>IM_N</signature_prefix>
		<file>results/f0914_walk_N0MABd_NSI.sto</file>
		<frame_delta>20</frame_delta>
		<Model type="Simbody">
			<model_file>f0914.osim</model_file>
			<state_init_file>f2354_init_walk_3_S10.sto</state_init_file>
			<!--state_init_optimization include_states="*" exclude_states="*_tx;*_ty;*_u" init_std="0.01" min="-0.5" max="0.5"/-->
			<integration_method>SemiExplicitEuler2</integration_method>
			<integration_accuracy>0.001</integration_accuracy>
			<use_fixed_control_step_size>1</use_fixed_control_step_size>
			<fixed_control_step_size>0.001</fixed_control_step_size>
			<sensor_delay_scaling_factor>1.0</sensor_delay_scaling_factor>
			<Controllers>
				<Controller type="NeuralController" delay_file="neural_delays_FEA.pn" activation_function="rectifier">
					<signature_prefix>N0MARG</signature_prefix>
					<SensorNeurons>
						<Sensor type="F" source="*"/>
						<!--Sensor type="L" source="*" offset="1" activation_function="linear"/-->
						<!--Sensor type="DP" source="pelvis_tilt" activation_function="linear"/-->
						<!--Sensor type="DV" source="pelvis_tilt" activation_function="linear"/-->
					</SensorNeurons>
					<!--PatternNeurons amount="1" period="1.25~0.02" t0="0.8~0.01" w0="0.3~0.03"/-->
					<InterNeurons amount="1" activation_function="gaussian" gain="1" mean="0.5~0.05" width="2.0~0.5"/>
					<MotorNeurons include="*" top_layer="0.0~0.5" monosynaptic=""/>
				</Controller>
			</Controllers>
		</Model>
	</Objective>
</Optimizer>

<?xml version="1.0" encoding="utf-8"?>
<Optimizer type="CmaOptimizer">
	<max_threads>32</max_threads>
	<thread_priority>-2</thread_priority>
	<lambda>0</lambda>
	<mu>0</mu>
	<sigma>1</sigma>
	<max_generations>1000</max_generations>

	<Objective type="SimulationObjective">
		<max_duration>1.0</max_duration>
		<Model type="Simbody">
			<model_file>models/jumper10dof24musc.osim</model_file>
			<Controllers>
				<Controller type="FeedForwardController">
					<function_type>PieceWiseLinear</function_type>
					<use_symmetric_actuators>1</use_symmetric_actuators>
					<control_points>4</control_points>
					<init_mean>0.5</init_mean>
					<init_std>0.25</init_std>
					<control_point_time_delta>0.2</control_point_time_delta>
					<optimize_control_point_time>0</optimize_control_point_time>
				</Controller>
				<Measure type="JumpingMeasure"/>
			</Controllers>
		</Model>
	</Objective>
</Optimizer>

<?xml version="1.0" encoding="utf-8"?>
<Optimizer type="CmaOptimizer">
	<init_file>initializations/f0918m_NMAUBOV_S10.par</init_file>
	<use_init_file>1</use_init_file>
	<init_file_std_factor>0.1</init_file_std_factor>
	<init_file_std_offset>0.01</init_file_std_offset>
	<use_init_file_std>0</use_init_file_std>
	<random_seed>123</random_seed>
	<Objective type="SimulationObjective">
		<max_duration>10</max_duration>
		<Model type="Simbody">
			<model_file>research/f0918m_k5s2_c2e6d10r4.osim</model_file>
			<state_init_file>f2354_init_walk_3_S15.sto</state_init_file>
			<state_init_optimization include_states="*" exclude_states="*_tx;*_ty;*_u" init_std="0.01" min="-0.5" max="0.5"/>
			<integration_method>SemiExplicitEuler2</integration_method>
			<integration_accuracy>0.001</integration_accuracy>
			<use_fixed_control_step_size>1</use_fixed_control_step_size>
			<fixed_control_step_size>0.001</fixed_control_step_size>
			<sensor_delay_scaling_factor>1.0</sensor_delay_scaling_factor>
			<Controllers>
				<Controller type="NeuralController" par_mode="virtual" delay_file="neural_delays_FEA.pn" delay_factor="1" activation="rectifier">
					<signature_prefix>NMSUBOV</signature_prefix>

					<SensorNeuronLayer>
						<SensorNeuron type="F" source="*" offset="0" activation="rectifier"/>
						<SensorNeuron type="L" source="*" offset="1" activation="linear"/>
						<SensorNeuron type="U" source="*" offset="0" activation="linear"/>
						<SensorNeuron type="DPV" source="pelvis_tilt" activation="linear"/>
					</SensorNeuronLayer>

					<MotorNeuronLayer>
						<MotorNeuron include="*" offset="0~0.01"/>
						<MotorNeuron input_layer="0" connect="monosynaptic" type="F" gain="0~0.01[-10,10]"/>
						<MotorNeuron input_layer="0" connect="monosynaptic" type="L" gain="0~0.01[-10,10]"/>
						<MotorNeuron input_layer="0" connect="synergetic" type="U" gain="0~0.01[-10,10]"/>
						<MotorNeuron input_layer="0" connect="ipsilateral" type="DPV" gain="0~0.1[-10,10]"/>
					</MotorNeuronLayer>

				</Controller>
				<INCLUDE file="measures/M_gait_S10.xml"/>
			</Controllers>
		</Model>
	</Objective>
</Optimizer>

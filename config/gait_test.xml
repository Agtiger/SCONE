<?xml version="1.0" encoding="utf-8"?>
<Optimizer type="CmaOptimizer">
	<name>Walk1024.FF.10F025.M2</name>
	<max_threads>32</max_threads>
	<thread_priority>-2</thread_priority>
	<lambda>0</lambda>
	<mu>0</mu>
	<sigma>1</sigma>
	<max_generations>5000</max_generations>
	<output_folder_base>../_SconeOutput/</output_folder_base>
	<maximize_objective>1</maximize_objective>

	<Objective type="SimulationObjective">
		<max_duration>3.0</max_duration>
		<Model type="Simbody">
			<model_file>models/gait1024.osim</model_file>
			<integration_accuracy>0.0001</integration_accuracy>
			<Controllers>
				<Controller type="FeedForwardController">
					<function_type>PieceWiseLinear</function_type>
					<use_symmetric_actuators>0</use_symmetric_actuators>
					<flat_extrapolation>0</flat_extrapolation>
					<control_points>6</control_points>
					<init_min>0.25</init_min>
					<init_max>0.75</init_max>
					<control_point_time_delta>0.25</control_point_time_delta>
					<optimize_control_point_time>0</optimize_control_point_time>
					<number_of_modes>2</number_of_modes>
					<init_mode_weight_min>-0.5</init_mode_weight_min>
					<init_mode_weight_max>0.5</init_mode_weight_max>
				</Controller>
				<Measure type="GaitMeasure">
					<termination_height>0.9</termination_height>
				</Measure>
			</Controllers>
		</Model>
	</Objective>
</Optimizer>

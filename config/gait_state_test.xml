<?xml version="1.0" encoding="utf-8"?>
<Optimizer type="CmaOptimizer">
	<name></name>
	<max_threads>32</max_threads>
	<thread_priority>-2</thread_priority>
	<lambda>0</lambda>
	<mu>0</mu>
	<sigma>1</sigma>
	<max_generations>5000</max_generations>
	<maximize_objective>1</maximize_objective>

	<Objective type="SimulationObjective">
		<signature_postfix>landing_threshold.T5</signature_postfix>
		<Model type="Simbody">
			<model_file>../models/f1024.osim</model_file>
			<state_init_file>../models/f2354_init_walk.sto</state_init_file>
			<pre_control_simulation_time>0.0</pre_control_simulation_time>
			<!--probe_class>Umberger2010MuscleMetabolicsProbe</probe_class-->
			<integration_accuracy>0.0001</integration_accuracy>
			<Controllers>
				<Controller type="GaitStateController">
					<landing_threshold init_mean="0.1" init_std="0.01" min="0.0" max="0.5" is_free="1"/>
					<ConditionalControllers>
						<ConditionalController>
							<Instances>
								<instance leg_index="0" phase_mask="0001"/>
								<instance leg_index="1" phase_mask="0001"/>
								<instance leg_index="0" phase_mask="0010"/>
								<instance leg_index="1" phase_mask="0010"/>
								<instance leg_index="0" phase_mask="0100"/>
								<instance leg_index="1" phase_mask="0100"/>
								<instance leg_index="0" phase_mask="1000"/>
								<instance leg_index="1" phase_mask="1000"/>
							</Instances>
							<Controller type="FeedForwardController">
								<use_symmetric_actuators>1</use_symmetric_actuators>
								<Function type="PieceWiseConstantFunction">
									<control_points>1</control_points>
									<control_point_y init_min="0.1" init_max="0.5" min="0.01" max="1"/>
									<!--control_point_delta_time mean="0.25" std="0.025" is_free="1"/-->
								</Function>
							</Controller>
						</ConditionalController>
					</ConditionalControllers>
				</Controller>
				<Measure type="GaitMeasure">
					<termination_height>0.9</termination_height>
					<min_velocity>0.5</min_velocity>
					<duration>5.0</duration>
				</Measure>
			</Controllers>
		</Model>
	</Objective>
</Optimizer>

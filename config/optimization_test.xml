<?xml version="1.0" encoding="utf-8"?>
<Optimization>
	<Objective>
		<Simulation type="Simbody">
			<Simbody>
				<model_file>models/jumper10dof24musc.osim</model_file>
				<accuracy>0.0001</accuracy>
				<max_time>2.0</max_time>
			</Simbody>
		</Simulation>
		<Controller type="FeedForwardController">
			<FeedForwardController type="PieceWiseLinear">
				<use_symmetric_actuators>true</use_symmetric_actuators>
				<ControlPoints>
					<count>3</count>
					<time_separation>0.1</time_separation>
					<fixed_time>true</fixed_time>
				</ControlPoints>
			</FeedForwardController>
		</Controller>
		<Measure type="Combined">
			<SubMeasures>
				<item>
					<Measure type="JumpingMeasure">
					</Measure>
					<weight>1.0</weight>
				</item>
				<item>
					<Measure type="EnergyMeasure">
						<Model type="SquaredTorques"/>
					</Measure>
					<weight>1.0</weight>
				</item>
			</SubMeasures>
		</Measure>
	</Objective>
	<Optimizer type="CMA">
		<lambda>0</lambda>
		<mu>0</mu>
		<sigma>1</sigma>
		<max_generations>1000</max_generations>
		<max_threads>4</max_threads>
	</Optimizer>
</Optimization>

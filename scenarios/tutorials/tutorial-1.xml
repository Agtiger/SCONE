<?xml version="1.0" encoding="utf-8"?>
<Optimizer type="CmaOptimizer">
	<max_generations>1000</max_generations>
	<maximize_objective>1</maximize_objective>
	<Objective type="SimulationObjective">
		<max_duration>1.0</max_duration>
		<Model type="Simbody">
			<model_file>f0914.osim</model_file>
			<integration_method>SemiExplicitEuler2</integration_method>
			<integration_accuracy>0.001</integration_accuracy>
			<Controllers>
				<Controller type="FeedForwardController">
					<use_symmetric_actuators>1</use_symmetric_actuators>
					<Function type="PieceWiseConstantFunction">
						<control_points>2</control_points>
						<control_point_y init_mean="0.25" init_std="0.1" min="0" max="1"/>
						<control_point_delta_time init_mean="0.2" init_std="0.02" is_free="1"/>
					</Function>
				</Controller>
				<Measure type="HeightMeasure">
					<target_body></target_body>
					<use_average_height>0</use_average_height>
					<terminate_on_peak>1</terminate_on_peak>
					<termination_height>0.5</termination_height>
					<max_admitted_counter_height>0.05</max_admitted_counter_height>
					<ignore_time>0</ignore_time>
					<upward_velocity_threshold>0.1</upward_velocity_threshold>
					<downward_velocity_threshold>-0.1</downward_velocity_threshold>
				</Measure>
			</Controllers>
		</Model>
	</Objective>
</Optimizer>
